{
  "extends": "@repo/typescript-config/base.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "noEmit": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "target": "ES2023",
    "lib": [
      "ES2023"
    ]
  },
  "include": [
    "index.ts"
  ],
  "exclude": [
    "node_modules",
    "generator"
  ]
}